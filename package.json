{
  "name": "blender-media",
  "version": "2.0.1",
  "description": "Blender Media Component",
  "main": "dist/index.js",
  "scripts": {
    "build": " npm run build:js; npm run build:css",
    "build:css": "postcss -c postcss.conf.js -o ./dist/css/media.css",
    "build:js": "NODE_ENV=production babel src -d dist",
    "dev": "concurrently \"npm run dev:js\" \"npm run dev:css\" \"npm run dev:serve\"",
    "dev:css": "postcss -c postcss.conf.js -o ./example/build/media.css -w",
    "dev:js": "NODE_ENV=development webpack-dev-server",
    "dev:serve": "node example/server.js",
    "lint": "eslint src",
    "prepublish": "npm run test; npm run build",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie-custom/blender-media.git"
  },
  "keywords": [
    "spatie",
    "blender",
    "javascript",
    "vue"
  ],
  "author": "Sebastian De Deyne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie-custom/blender-media/issues"
  },
  "homepage": "https://github.com/spatie-custom/blender-media",
  "dependencies": {
    "dragula": "^3.7.1",
    "dragula-constrain": "^1.0.0",
    "dropzone": "^4.3.0",
    "font-awesome-filetypes": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^2.0.0-rc.7",
    "vuex": "2.0.0-rc.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.25.0",
    "eslint": "^2.11.1",
    "express": "^4.13.4",
    "mocha": "^2.5.3",
    "multer": "^1.1.0",
    "postcss-cli": "^2.5.2",
    "postcss-easy-import": "^1.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^2.0.0",
    "vue": "^2.0.0-rc.7",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.0.3",
    "vuex": "2.0.0-rc.6",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.6"
  }
}
