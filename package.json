{
  "name": "blender-media",
  "version": "2.0.0-alpha.1",
  "description": "Blender Media Component",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "babel src -d dist -w",
    "build-example": "browserify src/index.js -o example/build/index.js -t [ babelify --presets [ es2015 ] ]",
    "dev-example": "watchify src/index.js -o example/build/index.js -t [ babelify --presets [ es2015 ] ]",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie-custom/blender-media.git"
  },
  "keywords": [
    "spatie",
    "blender",
    "javascript",
    "vue"
  ],
  "author": "Sebastian De Deyne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie-custom/blender-media/issues"
  },
  "homepage": "https://github.com/spatie-custom/blender-media",
  "peerDependencies": {
    "vue": "^1.0.24",
    "vuex": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "font-awesome-filetypes": "^1.0.1"
  }
}
