{
  "name": "blender-media",
  "version": "2.0.0-alpha.1",
  "description": "Blender Media Component",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist; css:build",
    "css:build": "postcss -c postcss.json -o ./dist/css/media.css",
    "css:dev": "postcss -c postcss.json -o ./example/build/media.css -w",
    "example:build": "browserify src/index.js -o example/build/index.js -t [ babelify --presets [ es2015 ] ]",
    "example:serve": "node example/server.js",
    "example:dev": "watchify src/index.js -o example/build/index.js -t [ babelify --presets [ es2015 ] ]",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie-custom/blender-media.git"
  },
  "keywords": [
    "spatie",
    "blender",
    "javascript",
    "vue"
  ],
  "author": "Sebastian De Deyne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie-custom/blender-media/issues"
  },
  "homepage": "https://github.com/spatie-custom/blender-media",
  "dependencies": {
    "dragula": "^3.7.1",
    "dragula-constrain": "^0.1.0",
    "font-awesome-filetypes": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^1.0.24",
    "vuex": "^0.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "dropzone": "^4.3.0",
    "eslint": "^2.11.1",
    "express": "^4.13.4",
    "multer": "^1.1.0",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-simple-vars": "^2.0.0",
    "vue": "^1.0.24",
    "vuex": "^0.6.3",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-syntax-object-rest-spread"
    ]
  }
}
