{
  "name": "blender-media",
  "version": "2.0.0-beta.4",
  "description": "Blender Media Component",
  "main": "dist/index.js",
  "scripts": {
    "build": " npm run build:js; npm run build:css",
    "build:css": "postcss -c postcss.conf.js -o ./dist/css/media.css",
    "build:js": "NODE_ENV=production babel src -d dist",
    "dev": "concurrently \"npm run dev:js\" \"npm run dev:css\" \"npm run dev:serve\"",
    "dev:css": "postcss -c postcss.conf.js -o ./example/build/media.css -w",
    "dev:js": "NODE_ENV=development watchify example/index.js -o example/build/index.js -t [ babelify --presets [ es2015 ] ]",
    "dev:serve": "node example/server.js",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie-custom/blender-media.git"
  },
  "keywords": [
    "spatie",
    "blender",
    "javascript",
    "vue"
  ],
  "author": "Sebastian De Deyne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie-custom/blender-media/issues"
  },
  "homepage": "https://github.com/spatie-custom/blender-media",
  "dependencies": {
    "dragula": "^3.7.1",
    "dragula-constrain": "^0.1.1",
    "dropzone": "^4.3.0",
    "font-awesome-filetypes": "^1.0.1"
  },
  "peerDependencies": {
    "vue": "^1.0.24",
    "vuex": "^0.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "concurrently": "^2.1.0",
    "eslint": "^2.11.1",
    "express": "^4.13.4",
    "mocha": "^2.5.3",
    "multer": "^1.1.0",
    "postcss-cli": "^2.5.2",
    "postcss-easy-import": "^1.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^2.0.0",
    "vue": "^1.0.25",
    "vuex": "^0.6.3",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-node-env-inline"
    ]
  }
}
